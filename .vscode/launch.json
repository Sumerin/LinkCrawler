{
   // Use IntelliSense to find out which attributes exist for C# debugging
   // Use hover for the description of the existing attributes
   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "configurations": [
        {
            "name": "Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Debug",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/LinkCrawler/bin/Debug/netcoreapp3.1/LinkCrawler.dll",
            "args": [],
            "cwd": "${workspaceFolder}/LinkCrawler",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Release",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Release",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/LinkCrawler/bin/Release/netcoreapp3.1/LinkCrawler.dll",
            "args": [],
            "cwd": "${workspaceFolder}/LinkCrawler",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Debug Test",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Test",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/LinkCrawler_Tests/bin/Debug/netcoreapp3.1/LinkCrawler_Tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}/LinkCrawler_Tests",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}